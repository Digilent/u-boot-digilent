/dts-v1/;
/include/ "mcf5282.dtsi"

/ {
	model = "BuS eb_cpuM5282";
	compatible = "bus,eb_cpuM5282";

	chosen {
		stdout-path = "serial0:115200n8";
	};
};

&uart0 {
	bootph-all;
	status = "okay";
};

&fec0 {
	status = "okay";
};

&i2c0 {
	status = "okay";

	rtc@68 {
		compatible = "dallas,ds1338";
		reg = <0x68>;
	};
};
