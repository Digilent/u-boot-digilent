/dts-v1/;

/ {
	#address-cells = <1>;
	#size-cells = <1>;

	binman {
		pre-load {
			content = <&image>;
			 algo-name = "sha256,rsa4096";
			 padding-name = "pkcs-1.5";
			 key-name = "tools/binman/test/230_dev.key";
			 header-size = <4096>;
			 version = <1>;
		};

		image: blob-ext {
			filename = "refcode.bin";
		};
	};
};
