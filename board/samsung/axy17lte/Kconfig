if TARGET_A5Y17LTE
config SYS_BOARD
	default "axy17lte"
	help
	  a5y17lte is a production board for SM-A520F phone on Exynos7880 SoC.

config SYS_VENDOR
	default "samsung"

config SYS_CONFIG_NAME
	default "exynos78x0-common"

config EXYNOS7880
    bool "Exynos 7880 SOC support"
    default y
endif

